import QtQuick 2.2

Item {
    id: banner
    height: 45
    property alias message: qMessage.text
    property string type

    Rectangle {
        id: background
        color: "#3949AB"//"#00CC6A"//"#388E3C"
        opacity: 0.8
        width: parent.width
        height: parent.height
        anchors.centerIn: parent
        smooth: true

        //        radius: 40
        Text {
            id: qMessage
            width: parent.width
            height: parent.height
            color: "white"
            font.pixelSize: 22
            wrapMode: Text.WordWrap
            renderType: Text.QtRendering
            anchors.left: parent.left
            anchors.leftMargin: 30
            verticalAlignment: Text.AlignVCenter
            horizontalAlignment: Text.AlignHCenter
        }

        Image {
            id: qImage
            source: "data:image/svg+xml;base64,"
            sourceSize.width: parent.height - 10
            sourceSize.height: parent.height - 10
            anchors.right: parent.right
            anchors.rightMargin: 10
            anchors.verticalCenter: parent.verticalCenter
        }
    }

    states: State {
        name: "portrait"
        PropertyChanges {
            target: banner
            height: 45
        }
    }

    Component.onCompleted: {
        qMessage.anchors.leftMargin = (background.width - qImage.width) / 2 - qMessage.width / 2
    }

    MouseArea {
        anchors.fill: parent
        onClicked: {
            messages.state = ""
        }
    }
